  "require": {
    "php": ">= 5.6.4",
    "symfony/event-dispatcher": "^5.0",
    "symfony/http-foundation": "^5.0.7 || ^6.0",
    "illuminate/config": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
    "illuminate/container": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
  },
  "autoload": {
    "psr-4": {
      "RFM\\": "src/"
